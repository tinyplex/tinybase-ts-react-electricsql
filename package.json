{
  "name": "tinybase-ts-react-electricsql",
  "version": "1.0.0",
  "author": "jamesgpearce",
  "license": "Apache-2.0",
  "main": "builder.js",
  "scripts": {
    "backend:start": "node ./backend/startCompose.js",
    "backend:stop": "docker compose --env-file backend/compose/.envrc -f backend/compose/docker-compose.yaml stop",
    "backend:up": "npm run backend:start -- --detach",
    "backend:down": "docker compose --env-file backend/compose/.envrc -f backend/compose/docker-compose.yaml down --volumes",
    "client:generate": "npm run electric:check && npx electric-sql generate --service http://localhost:5134 --proxy postgresql://prisma:proxy_password@localhost:65433/electric",
    "client:watch": "npm run client:generate -- --watch",
    "db:migrate": "node ./db/migrate.js",
    "db:psql": "node ./db/connect.js",
    "ports:configure": "node ./change-ports.js",
    "electric:start": "node ./backend/startElectric.js",
    "electric:check": "node ./check-electric-is-running.js",
    "build": "node copy-wasm-files.js && node builder.js",
    "build-prod": "cross-env NODE_ENV=production npm run build",
    "start": "npm run electric:check && cross-env SERVE=true npm run build"
  },
  "engines": {
    "node": ">=16.11.0"
  },
  "dependencies": {
    "electric-sql": "^0.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tinybase": "^4.6.1",
    "wa-sqlite": "rhashimoto/wa-sqlite#semver:^0.9.9"
  },
  "devDependencies": {
    "@databases/pg-migrations": "^5.0.2",
    "@electric-sql/prisma-generator": "^1.1.2",
    "@prisma/client": "5.8.0",
    "@types/node": ">=20.10.8",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "cross-env": "^7.0.3",
    "esbuild": "^0.16.17",
    "esbuild-plugin-inline-image": "^0.0.9",
    "fs-extra": "^11.2.0",
    "prisma": "5.8.0",
    "prompt": "^1.3.0",
    "shelljs": "^0.8.5",
    "tcp-port-used": "^1.0.2",
    "typescript": "^5.3.3"
  }
}